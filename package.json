{
  "name": "hypothetical-new-internal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "devSSL": "DEV_SSL=true nodemon",
    "build": "next build && yarn build:server",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "next lint",
    "test": "lint-staged",
    "ssl:generateCerts": "mkcert -install && mkdir certificates | true && mkcert -key-file certificates/key.pem -cert-file certificates/cert.pem localhost 127.0.0.1 ::1",
    "prisma:generate": "dotenv -c ${NODE_ENV:-development} -- prisma generate --schema ./lib/db/schema.prisma",
    "prisma:pull": "dotenv -c ${NODE_ENV:-development} -- prisma db pull --schema ./lib/db/schema.prisma",
    "prisma:migrateDev": "dotenv -c ${NODE_ENV:-development} -- prisma migrate dev --schema ./lib/db/schema.prisma",
    "prisma:migrateProd": "dotenv -c ${NODE_ENV:-development} -- prisma migrate deploy --schema ./lib/db/schema.prisma",
    "prisma:seed": "dotenv -c ${NODE_ENV:-development} -- prisma db seed",
    "prisma:studio": "dotenv -c ${NODE_ENV:-development} -- prisma studio",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prettify": "prettier --write .",
    "do": "SCRIPT=true dotenv -c ${NODE_ENV:-development} -- tsx scripts/$0.mts $*",
    "postinstall": "yarn prisma:generate && husky init",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx scripts/seed.mts",
    "schema": "lib/db/schema.prisma"
  },
